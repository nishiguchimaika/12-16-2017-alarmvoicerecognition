<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nishiguchimaika.voicerecognition2" >

    <uses-permission android:name="android.permission.WAKE_LOCK"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name=".SubActivity" android:label="@string/app_name">
        </activity>

        <activity android:name=".SubActivity2" android:label="@string/app_name">
        </activity>

        <activity android:name=".HowActivity" android:label="@string/app_name">
        </activity>

        <activity android:name=".NiActivity" android:label="@string/app_name">
        </activity>

        <activity android:name=".MuActivity" android:label="@string/app_name">
        </activity>

        <activity android:name=".Calculate" android:label="@string/app_name">
        </activity>

        <activity android:name=".Recognize" android:label="@string/app_name">
        </activity>

        <activity android:name=".Timer" android:label="@string/app_name">
        </activity>

        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true" >
        </service>
    </application>

</manifest>
